@model ImmovableProperty

<h2> @Model.District - @Model.Place</h2>

<div class="thumbnail">
    <img alt="@Model.District" src="@Model.Image" />
    <div class="caption-full">
        <h3 class="pull-right">@Model.Price</h3>
        <h3>
            <a href="#">@Model.Area</a>
        </h3>
        <h4>@Model.Street</h4>
        <p>@Model.Description</p>
    </div>
</div>